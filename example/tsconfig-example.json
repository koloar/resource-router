{
  "compilerOptions": {
    "outFile": "app/example.bundle.js",
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": false,
    "types": [
      "node"
    ]
  },
  "files": [
    "app/example.main.ts"
  ]
}
