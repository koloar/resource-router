{
  "name": "angular-resource-router",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebootstrap": "cpr src/lib/package.json dist/angular-resource-router/package.json -o",
    "bootstrap": "npm link dist/angular-resource-router && npm link angular-resource-router",
    "postbootstrap": "rimraf dist/",
    "ng": "ng",
    "start": "ng serve --proxy-config=proxy.conf.js",
    "prebuild": "ng lint",
    "build": "ng-packagr -p src/lib/ng-package.json && cpr README.md dist/angular-resource-router/README.md && cpr LICENSE dist/angular-resource-router/LICENSE",
    "postbuild": "ng test --watch=false",
    "buildapp": "ng build --target=production --env=prod --sourcemaps",
    "pretest": "ng-packagr -p src/lib/ng-package.json",
    "test": "ng test --browsers Chrome",
    "lint": "ng lint",
    "predocs": "rimraf dist/docs",
    "docs": "compodoc -p src/tsconfig.doc.json -d dist/docs --disablePrivate src/lib",
    "preghpages": "npm run docs",
    "ghpages": "gh-pages -d dist/docs",
    "e2e": "ng e2e",
    "ng-packagr": "ng-packagr"
  },
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.8",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@compodoc/compodoc": "1.0.5",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "6.0.60",
    "codelyzer": "4.1.0",
    "cpr": "3.0.1",
    "gh-pages": "1.1.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-detect-browsers": "2.2.6",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "2.0.0",
    "protractor": "5.1.2",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "tsickle": "0.26.0",
    "tslint": "5.9.1",
    "typescript": "2.5.3"
  }
}
