{
  "name": "angular-resource-router",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebootstrap": "cpr src/lib/package.json dist/angular-resource-router/package.json -o",
    "bootstrap": "npm link dist/angular-resource-router && npm link angular-resource-router",
    "postbootstrap": "rimraf dist/",
    "ng": "ng",
    "start": "ng serve --project angular-resource-router",
    "prebuild": "ng lint --project angular-resource-router && ng test --project angular-resource-router --watch=false",
    "build": "ng-packagr -p src/lib/ng-package.json && cpr README.md dist/angular-resource-router/README.md && cpr LICENSE dist/angular-resource-router/LICENSE",
    "buildapp": "ng build --project angular-resource-router-aot --prod --configuration=production --source-map",
    "test": "ng test --project angular-resource-router --browsers Chrome",
    "lint": "ng lint --project angular-resource-router",
    "predocs": "rimraf dist/docs",
    "docs": "compodoc -p src/tsconfig.doc.json -d dist/docs --disablePrivate --disableInternal src/lib",
    "preghpages": "npm run docs",
    "ghpages": "gh-pages -d dist/docs",
    "e2e": "ng e2e --project angular-resource-router",
    "ng-packagr": "ng-packagr"
  },
  "dependencies": {
    "@angular/animations": "7.2.4",
    "@angular/common": "7.2.4",
    "@angular/compiler": "7.2.4",
    "@angular/core": "7.2.4",
    "@angular/forms": "7.2.4",
    "@angular/http": "7.2.4",
    "@angular/platform-browser": "7.2.4",
    "@angular/platform-browser-dynamic": "7.2.4",
    "angular-in-memory-web-api": "0.9.0",
    "core-js": "2.4.1",
    "rxjs": "6.4.0",
    "tslib": "1.9.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.0",
    "@angular/cli": "7.3.0",
    "@angular/compiler-cli": "7.2.4",
    "@angular/language-service": "7.2.4",
    "@angular/router": "7.2.4",
    "@compodoc/compodoc": "1.0.5",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "6.0.60",
    "codelyzer": "4.5.0",
    "cpr": "3.0.1",
    "gh-pages": "1.1.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-detect-browsers": "2.2.6",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "1.0.4",
    "ng-packagr": "4.7.0",
    "protractor": "5.1.2",
    "rimraf": "2.6.2",
    "ts-node": "4.1.0",
    "tsickle": "0.34.0",
    "tslint": "5.12.1",
    "typescript": "3.2.4"
  }
}
