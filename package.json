{
  "name": "angular-resource-router",
  "version": "1.0.0-alpha.9",
  "homepage": "https://github.com/mdvorak/resource-router",
  "author": "Michal Dvorak",
  "license": "MIT",
  "description": "Angular router for RESTful clients",
  "main": "bundles/resource-router.umd.js",
  "jsnext:main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdvorak/resource-router.git"
  },
  "bugs": {
    "url": "https://github.com/mdvorak/resource-router/issues"
  },
  "keywords": [
    "angular",
    "angular2",
    "router",
    "rest"
  ],
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "clean": "gulp clean",
    "lint": "gulp tslint",
    "test": "karma start",
    "testonce": "karma start --single-run",
    "prestart": "npm run clean && tsc",
    "start": "concurrently \"tsc -w\" \"lite-server -c example/server.config.json\"",
    "build": "gulp build",
    "release": "gulp build && npm publish dist",
    "postpublish": "gulp clean",
    "preversion": "gulp clean && gulp tslint && karma start --single-run",
    "postversion": "npm run release && git push && git push --tags"
  },
  "dependencies": {
    "@angular/common": "^4.0.0-rc.3",
    "@angular/compiler": "^4.0.0-rc.3",
    "@angular/core": "^4.0.0-rc.3",
    "@angular/http": "^4.0.0-rc.3",
    "@angular/platform-browser": "^4.0.0-rc.3",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0-rc.3",
    "@angular/forms": "^4.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.3",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "codelyzer": "^3.0.0-beta.3",
    "concurrently": "^3.1.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-tslint": "^7.1.0",
    "gulp-uglify": "^2.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lite-server": "^2.2.2",
    "phantomjs-prebuilt": "^2.1.7",
    "pump": "^1.0.2",
    "rollup-stream": "^1.19.0",
    "run-sequence": "^1.2.2",
    "systemjs": "^0.19.41",
    "tslint": "^4.5.1",
    "typescript": "^2.1.5"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=4.0.0"
  }
