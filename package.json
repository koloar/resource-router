{
  "name": "angular-resource-router",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebootstrap": "cpr src/lib/package.json dist/angular-resource-router/package.json -o",
    "bootstrap": "npm link dist/angular-resource-router && npm link angular-resource-router",
    "postbootstrap": "rimraf dist/",
    "ng": "ng",
    "start": "ng serve --proxy-config=proxy.conf.json",
    "prebuild": "ng lint",
    "build": "ng-packagr && cpr README.md dist/angular-resource-router/README.md && cpr LICENSE.txt dist/angular-resource-router/LICENSE.txt",
    "postbuild": "ng test --watch=false",
    "buildapp": "ng build --target=production --env=prod --sourcemaps",
    "pretest": "ng-packagr",
    "test": "ng test",
    "lint": "ng lint",
    "predocs": "rimraf dist/docs",
    "docs": "compodoc -p src/tsconfig.doc.json -d dist/docs --disablePrivate src/lib",
    "e2e": "ng e2e",
    "ng-packagr": "ng-packagr"
  },
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "0.0.34",
    "@angular/cli": "1.4.9",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@compodoc/compodoc": "1.0.5",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "6.0.60",
    "ajv": "5.0.0",
    "codelyzer": "3.2.0",
    "cpr": "3.0.1",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-detect-browsers": "2.2.6",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.0.1",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-opera-launcher": "1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "1.0.0",
    "ng-packagr": "1.6.0",
    "protractor": "5.1.2",
    "rimraf": "2.6.2",
    "ts-node": "3.2.0",
    "tslint": "5.7.0",
    "typescript": "2.3.4"
  }
}
